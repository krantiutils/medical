{
  "name": "@swasthya/database",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./src/index.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "typecheck": "tsc --noEmit",
    "import:doctors": "tsx scripts/import-doctors.ts",
    "import:dentists": "tsx scripts/import-dentists.ts",
    "import:pharmacists": "tsx scripts/import-pharmacists.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.10.0",
    "csv-parse": "^6.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "prisma": "^5.10.0",
    "tsup": "^8.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
